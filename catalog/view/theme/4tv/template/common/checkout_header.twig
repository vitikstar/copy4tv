        <script>
            $(document).ready(function() {

                if (innerWidth < 1200) {
                    $('#search').appendTo('#dropdown-search-mob');

                    // info
                    $('.header-top').appendTo('#dropdown-info-mob .modal-body');
                    $('.header-phone-dropdown').appendTo('#dropdown-info-mob .modal-body');
                    $('.header-phone-dropdown').removeClass('dropdown-menu dropdown-menu-custom modal-custom');

                    var clone_1 =  $('.footer-social-wrap').clone();
                    var clone_2 =  $('.footer-social-follow-wrapper-main').clone();

                    clone_1.appendTo('#dropdown-info-mob .modal-body');
                    clone_2.appendTo('#dropdown-info-mob .modal-body');

                    $('#dropdown-info-mob .footer-column-title-1.footer-column-title').text('Следите за нами в соцсетях:');
                    // end info
                }

                $('.data-dropdown-link').click(function() {
                    var id = $(this).attr('data-dropdown');
                    if ($(id).css('display') == 'none') {

                        $('.data-dropdown-link').removeClass('active');
                        $(this).addClass('active');

                        $('.dropdown-custom-mob').hide();
                        $('.header-mob > .header-phone-tel-backdrop').remove();
                        $(id).show();
                        $('.header-mob').append('<div class="modal-backdrop in fade header-phone-tel-backdrop"></div>');
                    } else {

                        $('.data-dropdown-link').removeClass('active');


                        $('.dropdown-custom-mob').hide();
                        $('.header-mob > .header-phone-tel-backdrop').remove();
                    }
                });
            });

            $('.dropdown-custom-mob-close, .dropdown-login-reg-mob-btn-reg, .dropdown-login-reg-mob-btn-login').click(function() {
                $('.data-dropdown-link').removeClass('active');
                $('.dropdown-custom-mob').hide();
                $('.header-mob > .header-phone-tel-backdrop').remove();
            });

            $(document).click(function (e) { // отслеживаем событие клика по веб-документу
                if (innerWidth < 1200) {
                    var all_block = $('.dropdown-custom-mob, .data-dropdown-link');
                    var block = $('.dropdown-custom-mob'); // определяем элемент, к которому будем применять условия (можем указывать ID, класс либо любой другой идентификатор элемента)
                    if (!all_block.is(e.target) // проверка условия если клик был не по нашему блоку
                        && all_block.has(e.target).length === 0) { // проверка условия если клик не по его дочерним элементам
                        block.hide();
                        $('.header-mob > .header-phone-tel-backdrop').remove();

                        $('.data-dropdown-link').removeClass('active');
                        // если условия выполняются - скрываем наш элемент
                    }
                }
            });
        </script>
