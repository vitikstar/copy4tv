<form class="form-horizontal" id="form-art_aqa_product">
	{% if art_aqa_product_form['f_name'] %}
	<div class="form-group {{ art_aqa_product_form['f_name']['required'] == 2 ? 'required' }}">
	  <div class="col-sm-12">
	    <label class="control-label" for="input-f_name">{{ art_aqa_product_form['f_name']['title'] }}</label>
	    <input type="text" name="f_name" value="{{ art_aqa_product_form['f_name']['value'] }}" id="input-f_name" class="form-control" />
	  </div>
	</div>
	{% endif %}
	{% if art_aqa_product_form['f_select'] %}
		{% set class = "col-sm-6" %}
		{% set div = "" %} 
	{% else %}
		{% set class = "col-sm-12" %}
		{% set div = "form-group" %}
	{% endif %}
	{% if art_aqa_product_form['f_select'] %}
	<div class="form-group">
		<div class="col-sm-12"><h4>{{ art_aqa_product_form['f_select']['title'] }}</h4></div>
	{% endif %}
	{% if art_aqa_product_form['f_telephone'] %}
	<div class="{{ div }} {{ art_aqa_product_form['f_telephone']['required'] == 2 ? 'required' }}">
	  <div class="{{ class }}">
	    <label class="control-label" for="input-f_telephone">{{ art_aqa_product_form['f_telephone']['title'] }}</label>
	    <input type="tel" name="f_telephone" value="{{ art_aqa_product_form['f_telephone']['value'] }}"  id="input-f_telephone" class="form-control" />
	  </div>
	</div>
	{% endif %}
	{% if art_aqa_product_form['f_email'] %}
	<div class="{{ div }} {{ art_aqa_product_form['f_email']['required'] == 2 ? 'required' }}">
	  <div class="{{ class }}">
	    <label class="control-label" for="input-f_email">{{ art_aqa_product_form['f_email']['title'] }}</label>
	    <input type="email" name="f_email" value="{{ art_aqa_product_form['f_email']['value'] }}"   id="input-f_email" class="form-control" />
	  </div>
	</div>
	{% endif %}
	{% if art_aqa_product_form['f_select'] %}
	</div>
	{% endif %}





	<div class="form-group required">
		<div class="col-sm-12">
			<label class="control-label" for="input-f_email">{{ text_theme_aqa }}</label>
			<select class="ask-modal-select pr-review-sort-select form-control" name="theme">
				<option value="{{ text_theme_service }}" selected="selected">{{ text_theme_service }}</option>
				<option value="{{ text_theme_garatue }}">{{ text_theme_garatue }}</option>
				<option value="{{ text_theme_teh }}">{{ text_theme_teh }}</option>
			</select>
		</div>
	</div>




	{% if art_aqa_product_form['f_question'] %}
	<div class="input-f_question_wrap form-group {{ art_aqa_product_form['f_question']['required'] == 2 ? 'required' }}">
	  <div class="col-sm-12">
	    <label class="control-label" for="input-f_question">{{ art_aqa_product_form['f_question']['title'] }}</label>
	    <textarea name="f_question" rows="5" id="input-f_question" class="form-control"></textarea>
	  </div>
	</div>
	{% endif %}
	{% if art_aqa_product_form['f_communication'] %}
	<div class="form-group">
        <label class="col-sm-12 control-label" for="input-communication">{{ art_aqa_product_form['f_communication']['title'] }}</label>
        <br>
        <div class="col-sm-12 input-communication art_communication">
