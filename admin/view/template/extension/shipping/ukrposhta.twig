				      								<input type="radio" name="shipping_ukrposhta[debugging_mode]" value="1" id="input-debugging_mode-enabled"{{ ukrposhta['debugging_mode'] ? ' checked' }}><label class="col-sm-4" for="input-debugging_mode-disabled">{{ text_enabled }}</label>
				      							</div>
