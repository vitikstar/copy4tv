				      								<input type="radio" name="shipping_ukrposhta_status" value="1" id="input-status-enabled"{{ ukrposhta_status ? ' checked' }}><label class="col-sm-4" for="input-status-disabled">{{ text_enabled }}</label>
				      							</div>
				      						</div>
